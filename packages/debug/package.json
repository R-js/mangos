{
  "name": "@mangos/debug",
  "version": "0.0.9-rc5",
  "description": "super tiny and fast logger inspired by debug",
  "repository": {
    "directory": "./packages/debug",
    "type": "git",
    "url": "https://github.com/R-js/mangos.git"
  },
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/commonjs/index.cjs"
  },
  "types": "./dist/types/index.d.ts",
  "sideEffects": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.18.1"
  },
  "files": [
    "dist",
    "README.md",
    "license.md",
    "screenshot-01.png",
    "screenshot-02.png"
  ],
  "scripts": {
    "test": "npm run test-nodejs",
    "test-nodejs": "jest --env node",
    "test-web": "jest --env jsdom",
    "build": "node scripts/build.mjs",
    "prepublishOnly": "npm run build",
    "prettier-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "precommit": "lint-staged"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "maxWorkers": "50%",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "fixture.ts",
      "test"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "devDependencies": {
    "@mangos/jxpath": "1.0.13",
    "@types/jest": "27.0.1",
    "@vercel/style-guide": "3.0.0",
    "escodegen": "2.0.0",
    "eslint": "8.12.0",
    "husky": "7.0.2",
    "jest": "27.1.1",
    "lint-staged": "11.1.2",
    "prettier": "2.6.2",
    "ts-jest": "27.0.5",
    "ts-jest-resolver": "2.0.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "ms": "3.0.0-canary.1"
  }
}
